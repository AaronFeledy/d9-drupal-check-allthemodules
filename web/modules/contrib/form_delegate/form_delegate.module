<?php

/**
 * @file
 * Implements hooks.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function form_delegate_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.form_delegate':
      return '<p>' . t('This module provides plugins for developers to alter forms and entity forms. For proper documentation see the the README.') . '</p>';
  }
}
