<?php

/**
 * @file
 * Contains html_diff.module
 */

use Drupal\Core\Routing\RouteMatchInterface;
use Drupal\Core\Entity\EntityInterface;
use Drupal\Core\Entity\Display\EntityViewDisplayInterface;
use Drupal\node\NodeInterface;
use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_help().
 */

function html_diff_help($route_name, RouteMatchInterface $route_match)
{
  switch ($route_name) {
    // Main module help for the html_diff module.
    case 'help.page.html_diff':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('HTML DIFF') . '</p>';
      return $output;
    break;
  }
}

/**
 * Implements hook_theme().
 */

function html_diff_theme()
{
  $theme = [];

  $theme['node_diff'] = array(
    'render element' => 'elements');

  return $theme;
}

function template_preprocess_node_diff(&$variables) {
  template_preprocess_node($variables);
}
