<?php

/**
 * @file
 * Contains leage_oauth_login.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function leage_oauth_login_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the leage_oauth_login module.
    case 'help.page.leage_oauth_login':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('League Oauth login') . '</p>';
      return $output;

    default:
  }
}
