<?php

/**
 * @file
 * Contains commerce_wallet.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function commerce_wallet_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the commerce_wallet module.
    case 'help.page.commerce_wallet':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Integrate Wallet Module With Drupal Commerce 2.x') . '</p>';
      return $output;

    default:
  }
}
