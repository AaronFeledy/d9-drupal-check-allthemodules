<?php

/**
 * @file
 * Contains inherit_link_ui.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function inherit_link_ui_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the inherit_link_ui module.
    case 'help.page.inherit_link_ui':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Inherit Link UI') . '</p>';
      return $output;

    default:
  }
}
