<?php

/**
 * @file
 * Contains pe_event.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function pe_event_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the pe_event module.
    case 'help.page.pe_event':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Announcements &amp; Events') . '</p>';
      return $output;

    default:
  }
}

