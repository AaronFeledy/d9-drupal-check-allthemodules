<?php

/**
 * @file
 * Contains install, uninstall, and update functions for Omeda.
 */

/**
 * Implements hook_uninstall().
 */
function omeda_uninstall() {
  $config = \Drupal::service('config.factory');

  /** @var \Drupal\Core\Config\Config $smtp_config */
  $smtp_config = $config->getEditable('omeda.settings');
  $smtp_config->delete();
}
