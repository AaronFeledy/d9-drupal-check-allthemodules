<?php

/**
 * @file
 * Contains entity_reference_crud_display.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function entity_reference_crud_display_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the entity_reference_crud_display module.
    case 'help.page.entity_reference_crud_display':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Load entities by ajax') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function entity_reference_crud_display_theme() {
  return [
    'entity_reference_crud_display' => [
      'render element' => 'children',
    ],
  ];
}
