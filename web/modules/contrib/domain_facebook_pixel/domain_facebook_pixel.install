<?php

/**
 * @file
 * Install, update and uninstall functions for the Domain Facebook Pixel module.
 */

/**
 * Implements hook_uninstall().
 *
 * Remove domain_facebook_pixel configurations on uninstall.
 */
function domain_facebook_pixel_uninstall() {
  \Drupal::configFactory()->getEditable('domain_facebook_pixel.settings')->delete();
}
