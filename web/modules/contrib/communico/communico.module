/**
 * Implements hook_theme().
 */
function communico_theme($existing, $type, $theme, $path) {
  $variables = [
    'communico_block' => [
      'variables' => [
        'events' => NULL,
      ],
      'template' => 'communico_block',
    ],
    'communico_item' => [
      'variables' => [
        'title_link' => NULL,
        'start_date' => NULL,
        'end_date' => NULL,
        'location' => NULL,
        'room' => NULL,
      ],
      'template' => 'communico_item',
    ],
  ];
  return $variables;
}
