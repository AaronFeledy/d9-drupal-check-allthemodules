<?php

/**
 * @file
 * Contains druminate.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function druminate_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the druminate module.
    case 'help.page.druminate':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Drupal wrapper for the Luminate Api.') . '</p>';
      return $output;

    default:
  }
}
