<?php

use Drupal\sender\Entity\Message;

/**
 * Add an administrative label to existing messages.
 */
function sender_update_8101(&$sandbox) {
  foreach (Message::loadMultiple() as $message) {
    // Uses the subject as the label.
    $label = $message->getSubject();
    $message->setLabel($label);
    $message->save();
  }
}
