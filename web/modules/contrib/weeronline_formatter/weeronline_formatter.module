<?php

/**
 * @file
 * Defines simple WeerOnline formatter for the id field.
 */

/**
 * Implements hook_theme().
 */
function weeronline_formatter_theme() {
  return [
    'weeronline_id_formatter' => [
      'variables' => [
        'weeronline_id' => NULL,
        'temperature_scale' => NULL,
      ],
      'template' => 'weeronline-id-formatter',
    ],
  ];
}
