<?php

/**
 * @file
 * Contains masked_output.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function masked_output_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'masked_output.settings':
      return '<p>' . t('Masked Output Settings.') . '</p>';
  }
}
