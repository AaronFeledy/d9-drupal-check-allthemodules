<?php

/**
 * @file
 * Module file for the MaxCDN Cache module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help()
 */
function max_cdn_cache_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the MaxCDN Cache module.
    case 'help.page.max_cdn_cache':
      $output = check_markup(file_get_contents(drupal_get_path('module', 'max_cdn_cache') . '/README.txt'));
      return $output;
  }
}
