<?php

/**
 * @file
 * Webform plugin for Address element with Crafty Clicks Postcode lookup.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function webform_craftyclicks_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.webform_craftyclicks':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Webform plugin for Address element with Crafty Clicks Postcode lookup.') . '</p>';
      return $output;

    default:
  }
}
