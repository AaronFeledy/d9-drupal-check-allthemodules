<?php

function meu_paragraphs_uninstall() {
  $query = \Drupal::database()->delete('media_usage', array('submodule' => 'meu_paragraphs'));
  $result = $query->execute();
  \Drupal::logger('meu_paragraphs')->info('Media entity usage for Paragraphs uninstalled. Deleted @count entries.', ['@count' => $result]);
}