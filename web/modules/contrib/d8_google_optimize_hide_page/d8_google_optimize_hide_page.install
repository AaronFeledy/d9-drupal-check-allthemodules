<?php

/**
 * @file
 * Installation file for Google Optimize hide page module.
 */

/**
 * Implements hook_uninstall().
 *
 * Remove config if module is uninstalled.
 */
function d8_google_optimize_hide_page_uninstall() {
  Drupal::configFactory()->getEditable('d8_google_optimize_hide_page.settings')->delete();
}
