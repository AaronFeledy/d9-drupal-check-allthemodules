<?php

/**
 * @file
 * Contains bulk_term_merge.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function bulk_term_merge_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the bulk_term_merge module.
    case 'help.page.bulk_term_merge':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Merge Terms in Bulk') . '</p>';
      return $output;

    default:
  }
}
