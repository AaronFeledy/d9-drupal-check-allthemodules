<?php

/**
 * @file
 * Update hooks for campaignmonitor.
 */

/**
 * Remove the library_path config.
 */
function campaignmonitor_update_8001() {
  $config = \Drupal::configFactory()->getEditable('campaignmonitor.settings');
  $data = $config->getRawData();
  unset($data['library_path']);
  $config->setData($data)->save();
}
