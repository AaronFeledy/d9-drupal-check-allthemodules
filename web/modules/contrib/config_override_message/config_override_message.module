use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function config_override_message_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'config_override_message.overview':
      return '<p>' . t('List of configuration overrides provided by site wide override folder and modules specific configuration overrides.') . '</p>';

  }
}
