<?php

/**
 * @file
 * Install file for text2image.
 */

/**
 * Implements hook_uninstall().
 */
function text2image_install() {
  \Drupal::service('text2image.fonts')->restoreDefaults();
}

/**
 * Implements hook_uninstall().
 */
function text2image_uninstall() {
  \Drupal::configFactory()->getEditable('text2image.settings')->delete();
}
