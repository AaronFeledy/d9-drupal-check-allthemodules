<?php

/**
 * @file
 * Contains sms_vbo.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function sms_vbo_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the sms_vbo module.
    case 'help.page.sms_vbo':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Intergate sending SMS with views bulk operations') . '</p>';
      return $output;

    default:
  }
}
