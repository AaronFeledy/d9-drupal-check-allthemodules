{
    "name": "drupal/moment",
    "type": "drupal-module",
    "description": "Integrates Moment.js library into Drupal.",
    "keywords": [
        "Drupal",
        "Moment"
    ],
    "license": "GPL-2.0+",
    "homepage": "https://www.drupal.org/project/moment",
    "support": {
        "issues": "http://drupal.org/project/issues/moment",
        "source": "http://cgit.drupalcode.org/moment"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin",
        "preferred-install": "dist"
    },
    "repositories": {
        "drupal-8": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "drupal/system_stream_wrapper": "1.x-dev"
    }
}
