{
    "name": "drupal/prometheus_exporter",
    "description": "A Prometheus exporter for Drupal",
    "type": "drupal-module",
    "keywords": ["Drupal", "Prometheus"],
    "homepage": "http://drupal.org/project/prometheus_exporter",
    "license": "GPL-2.0+",
    "support": {
        "issues": "https://www.drupal.org/project/issues/prometheus_exporter",
        "source": "https://github.com/previousnext/prometheus_exporter"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin/",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "previousnext/php-prometheus": "dev-master"
    },
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9"
            }
        }
    }
}
