{
    "name": "drupal/link_trail_by_path",
    "description": "Link Trail By Path sets the active-trail on any link items according to the current url, or any link generated using the l() API function.",
    "type": "drupal-module",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "homepage": "https://drupal.org/project/link_trail_by_path",
    "authors": [
        {
            "name": "Pol Dellaiera (@drupol)",
            "homepage": "https://www.drupal.org/u/pol",
            "role": "Maintainer"
        }
    ],
    "require-dev": {
        "drupol/drupal-conventions": "^1"
    },
    "scripts": {
        "grumphp": "./vendor/bin/grumphp run"
    },
    "extra": {
        "grumphp": {
            "config-default-path": "vendor/drupol/drupal-conventions/config/drupal8/grumphp.yml"
        }
    }
}
