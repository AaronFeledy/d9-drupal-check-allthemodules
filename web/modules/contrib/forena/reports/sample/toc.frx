<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
	<title>Sample reports and graphs</title>
	<frx:category>Help</frx:category>
	<frx:options hidden="0" skin="sample"/>
	<frx:access>drupal.access forena samples</frx:access>
	<frx:parameters>
	</frx:parameters>
	<frx:docgen>
	</frx:docgen>
	<frx:fields>
		<frx:field id="title" link="reports/sample.{link}/nojs/sample-report/html"  class="use-ajax"></frx:field>
	</frx:fields>
	<frx:menu enabled="1" path="reports/samples" type="normal-item" title="Reporting Samples" weight="20" menu_name="navigation" />
	<frx:cache />
</head>
<body>
	<div class="frx-block" id="topics" frx:block="sampledb/sample_topics">
		<div>
			<ul>
				<li id="{link}" frx:foreach="//chapter">{title} <span frx:if="{subtitle}&amp;{parm.show_subtitle}">-
						{subtitle}.</span>
					<blockquote frx:if="{abstract}&amp;{parm.show_abstract}">{abstract}</blockquote>
				</li>
			</ul>
		</div>
	</div>
  <div id="sample-report">
		&nbsp;
	</div>
</body>
</html>