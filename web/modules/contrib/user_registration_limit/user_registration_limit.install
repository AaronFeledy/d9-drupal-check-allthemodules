<?php

/**
 * @file
 * Install file.
 */

/**
 * Implements hook_install().
 */
function user_registration_limit_install() {
  $config = \Drupal::configFactory()->getEditable('user.settings');
  $config->set('user_registration_limit', NULL)->save(TRUE);
}

/**
 * Implements hook_uninstall().
 */
function user_registration_limit_uninstall() {
  $config = \Drupal::configFactory()->getEditable('user.settings');
  $config->set('user_registration_limit', NULL)->save(TRUE);
}
