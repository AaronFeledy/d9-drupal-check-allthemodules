<?php

/**
 * @file
 * Contains normalize_address.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function normalize_address_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the icon_field module.
    case 'help.page.normalize_address':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provides an ability to add field with address returned by Google API') . '</p>';
      $output .= '<p>' . t('Please read Readme file or visit https://www.drupal.org/project/normalize_address') . '</p>';
      return $output;

    default:
  }
}
