<?php

/**
 * @file
 * Contains field_presets.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function field_presets_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the field_presets module.
    case 'help.page.field_presets':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Quickly create a field from predefined preset.') . '</p>';
      return $output;

    default:
  }
}
