<?php

/**
 * @file
 * Contains tetw.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function tetw_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the tetw module.
    case 'help.page.tetw':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Creates a field widget that uses Taxonomy to present select list options.') . '</p>';
      return $output;

    default:
  }
}
