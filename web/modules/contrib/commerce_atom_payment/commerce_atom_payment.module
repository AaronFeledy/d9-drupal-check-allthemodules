<?php

/**
 * @file
 * Contains commerce_atom_payment.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function commerce_atom_payment_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the commerce_atom_payment module.
    case 'help.page.commerce_atom_payment':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Integrates Atom Payment Gateway with Drupal Commerce.') . '</p>';
      return $output;

    default:
  }
}
