<?php

/**
 * @file
 * Install, update and uninstall functions for the Entity Edit Redirect module.
 */

/**
 * Sets default destination querystring.
 */
function entity_edit_redirect_update_8001() {
  \Drupal::configFactory()->getEditable('entity_edit_redirect.settings')
    ->set('destination_querystring', 'destination')
    ->save();
}
