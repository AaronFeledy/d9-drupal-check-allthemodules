<?php

/**
 * @file
 * Contains visualn_commerce.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function visualn_commerce_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the visualn_commerce module.
    case 'help.page.visualn_commerce':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Graphical representation for sales and other commerce data') . '</p>';
      return $output;

    default:
  }
}
