/**
 * Add term to taxonomy.
 */
function _currency_taxonomy_add_terms() {
  // Get Currency list.
  $currency_data_list = array();
  $currency_data_list = _currency_taxonomy_get_currency_data();
  if (isset($currency_data_list) && !empty($currency_data_list)) {
    foreach ($currency_data_list as $currency_data) {
      $term = entity_create('taxonomy_term', array(
        'name' => $currency_data['currency'],
        'vid' => 'currency',
        'vocabulary_machine_name' => 'currency',
        'field_currency_iso' => $currency_data['iso_code'],
        'field_currency_country' => $currency_data['country'],
      ));
      $term->save();
    }
  }
}

/**
 * Create a taxonomy and attach a field to it.
 */
function _currency_taxonomy_create_taxonomy() {
  $vocabulary = entity_create('taxonomy_vocabulary', array(
    'name' => 'Currency',
    'vid' => 'currency',
    'machine_name' => 'currency',
    'description' => 'Currency list with ISO code and country.',
    'hierarchy' => 1,
    'weight' => 1,
  ));
  $vocabulary->save();
  // Create a field.
  // Add a field to the vocabulary.
  entity_create('field_storage_config', array(
    'field_name' => 'field_currency_iso',
    'entity_type' => 'taxonomy_term',
    'type' => 'string',
  ))->save();
  entity_create('field_config', array(
    'field_name' => 'field_currency_iso',
    'entity_type' => 'taxonomy_term',
    'label' => t('Iso label'),
    'bundle' => 'currency',
    'description' => t('Currency iso code'),
    'required' => TRUE,
  ))->save();
  // Assign widget settings for the 'default' form mode .
  entity_get_form_display('taxonomy_term', 'currency', 'default')
    ->setComponent('field_currency_iso', array(
      'type' => 'string_textfield',
    ))->save();
  // Assign display settings for the 'default' and 'teaser' view modes.
  entity_get_display('taxonomy_term', 'currency', 'default')
    ->setComponent('field_currency_iso', array(
      'label' => 'hidden',
      'type' => 'string',
    ))->save();
  entity_create('field_storage_config', array(
    'field_name' => 'field_currency_country',
    'entity_type' => 'taxonomy_term',
    'type' => 'string',
  ))->save();
  entity_create('field_config', array(
    'field_name' => 'field_currency_country',
    'entity_type' => 'taxonomy_term',
    'bundle' => 'currency',
    'label' => t('Country'),
    'description' => t('Currency country'),
    'required' => TRUE,
  ))->save();
  // Assign widget settings for the 'default' form mode.
  entity_get_form_display('taxonomy_term', 'currency', 'default')
    ->setComponent('field_currency_country', array(
      'type' => 'string_textfield',
    ))->save();
  // Assign display settings for the 'default' and 'teaser' view modes.
  entity_get_display('taxonomy_term', 'currency', 'default')
    ->setComponent('field_currency_country', array(
      'label' => 'hidden',
      'type' => 'string',
    ))->save();
}

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function currency_taxonomy_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.currency_taxonomy':
      $output = '';
      $output .= '<p>' . t('This module helps to provide currency taxonomy with country and ISO code. It provide list of all available currency.') . '</p>';
      $output .= '<p>' . t("Each list term name has value in the 'Currency Name(ISO code)' format where ISO code is Alphabetic ISO code of that currency.") . '</p>';
      return $output;
  }
}
