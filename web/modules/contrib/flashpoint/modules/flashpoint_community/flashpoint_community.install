<?php
/**
 * @file
 */

use Drupal\Core\Config\ConfigFactoryOverrideInterface;

/**
 * Implements hook_install().
 *
 */
function flashpoint_community_install() {
  \Drupal::service('config.installer')->installDefaultConfig('module', 'flashpoint_community');
}

/**
 * Implements hook_uninstall().
 */
function flashpoint_community_uninstall() {

  \Drupal::service('config.manager')->uninstall('module', 'flashpoint_community');
}