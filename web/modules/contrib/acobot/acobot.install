<?php

/**
 * @file
 * Install, update and uninstall functions for the acobot module.
 */

/**
 * Implements hook_uninstall().
 */
function acobot_uninstall() {
  \Drupal::state()->delete('acobot_visibility_roles'); 
  \Drupal::state()->delete('acobot_token'); 
  \Drupal::state()->delete('acobot_roles'); 
  \Drupal::state()->delete('acobot_pages'); 
  \Drupal::state()->delete('acobot_visibility'); 
}
