<?php

/**
 * Implements hook_page_attachments().
 *
 * Adds the quickedit library to the page for any user who has the 'access
 * in-place editing' permission.
 */
function da11y_page_attachments(array &$page) {
  $page['#attached']['library'][] =  'da11y/da11y';
  $page['#attached']['drupalSettings']['da11y_setting'] = \Drupal::config('da11y.settings')->get('settings');
}