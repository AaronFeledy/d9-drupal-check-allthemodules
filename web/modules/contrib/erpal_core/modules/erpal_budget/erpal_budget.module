<?php

use \Drupal\erpal_budget\Entity\ErpalBudgetType;

/**
 * Menu argument loader. Returns a erpal_budget_type entity
 *
 * @param $id
 * @return \Drupal\Core\Entity\EntityInterface|static
 */
function erpal_budget_type_load($id) {
  return ErpalBudgetType::load($id);
}

/**
 * Implements hook_theme().
 */
function erpal_budget_theme() {
  return array(
    'erpal_budget_add_list' => array(
      'variables' => array('content' => NULL),
      'file' => 'erpal_budget.pages.inc',
    ),
  );
}
