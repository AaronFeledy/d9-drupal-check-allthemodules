<?php

/**
 * @file
 * Module installation file.
 */

/**
 * Implements hook_install().
 */
function vk_authentication_install() {
  \Drupal::messenger()
    ->addMessage(t('Your Module settings are available under <a href =":path">Configuration->People->Vk authentication settings.</a>',
      [':path' => 'config/people/vk_authentication']));
}
