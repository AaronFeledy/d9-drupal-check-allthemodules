{
  "name": "drupal/entity_normalization",
  "description": "Provides a way to easily create entity normalizing definitions in YAML files.",
  "type": "drupal-module",
  "homepage": "https://drupal.org/project/entity_normalization",
  "authors": [
    {
      "name": "Remko Klein",
      "homepage": "https://www.drupal.org/u/remkoklein",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://drupal.org/project/issues/entity_normalization",
    "source": "https://cgit.drupalcode.org/entity_normalization"
  },
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "drupal/core": "^8.5",
    "drupal/coder": "^8.2",
    "phpunit/phpunit": "^4.8"
  },
  "scripts": {
    "test": [
      "@phpcs-dp",
      "@phpcs",
      "@phpunit"
    ],
    "phpcs": "vendor/bin/phpcs -p  --ignore=vendor/ --standard=vendor/drupal/coder/coder_sniffer/Drupal .",
    "phpcs-dp": "vendor/bin/phpcs -p  --ignore=vendor/ --standard=vendor/drupal/coder/coder_sniffer/DrupalPractice .",
    "phpunit": "vendor/bin/phpunit -c tests/phpunit.xml --coverage-text"
  }
}
