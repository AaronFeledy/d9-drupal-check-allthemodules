use Drupal\Core\Routing\RouteMatchInterface;


/**
 * Implements hook_help().
 */
function slicky_mobile_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.slicky_mobile':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Slicky mobile menu') . '</p>';
      return $output;

    default:
  }
}


/**
 * Implements hook_page_attachments().
 */
function slicky_mobile_page_attachments(array &$attachments) {

  $attachments['#attached']['library'][] = 'slicky_mobile/slicky-mobile';
}





