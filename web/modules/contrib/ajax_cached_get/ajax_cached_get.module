<?php

/**
 * @file
 * Contains ajax_cached_get.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function ajax_cached_get_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the ajax_cached_get module.
    case 'help.page.ajax_cached_get':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Allow ajax get requests to be cached') . '</p>';
      return $output;

    default:
  }
}
