<?php

/**
 * @file
 * Module File for Blackbaud SKY API.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function blackbaud_sky_api_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help.
    case 'help.page.blackbaud_sky_api':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provides OAuth connectivity with the Blackbaud SKY API.') . '</p>';

      return $output;
  }
}
