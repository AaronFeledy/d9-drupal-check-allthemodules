<?php

/**
 * @file
 * Contains pagedesigner_link.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function pagedesigner_link_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the pagedesigner_link module.
    case 'help.page.pagedesigner_link':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Pagedesigner link extension') . '</p>';
      return $output;

    default:
  }
}
