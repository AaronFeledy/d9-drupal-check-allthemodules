<?php

/**
 * @file
 * Contains pagedesigner_embed.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function pagedesigner_embed_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the pagedesigner_embed module.
    case 'help.page.pagedesigner_embed':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Pagedesigner embed integration') . '</p>';
      return $output;
    default:
  }
}
