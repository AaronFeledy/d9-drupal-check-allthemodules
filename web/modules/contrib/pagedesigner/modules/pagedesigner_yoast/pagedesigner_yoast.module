<?php

/**
 * @file
 * Contains pagedesigner_yoast.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function pagedesigner_yoast_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the pagedesigner_yoast module.
    case 'help.page.pagedesigner_yoast':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Pagedesigner yoast integration') . '</p>';
      return $output;
    default:
  }
}
