<?php

/**
 * @file
 * Contains pagedesigner_gallery.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function pagedesigner_gallery_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the pagedesigner_gallery module.
    case 'help.page.pagedesigner_gallery':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Pagedesigner gallery extension') . '</p>';
      return $output;

    default:
  }
}
