use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function httpcsvalidation_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the httpcsvalidation module.
    case 'help.page.httpcsvalidation':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('HTTPCS Website validation') . '</p>';
      return $output;

    default:
  }
}

