<?php

/**
 * @file
 * Primary module hooks for Group Media Plus Upload Attacher module.
 */

use Drupal\Core\Entity\EntityInterface;
use Drupal\Core\Form\FormStateInterface;
use Drupal\groupmediaplus_upload\Hooks;

/**
 * Implements hook_form_alter().
 */
function groupmediaplus_upload_form_alter(&$form, \Drupal\Core\Form\FormStateInterface $formState, $formId) {
  Hooks::uploadFormAlter($form, $formState, $formId);
}

/**
 * Implements hook_entity_insert().
 */
function groupmediaplus_upload_entity_insert(EntityInterface $entity) {
  Hooks::entityInsert($entity);
}
