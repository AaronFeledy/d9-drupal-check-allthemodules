<?php

/**
 * @file
 * Contains components_extras.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function components_extras_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the components_extras module.
    case 'help.page.components_extras':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provide components plugin and an associated render element for developers') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function components_extras_theme() {
  return [
    'components_extras' => [
      'render element' => 'element',
    ],
  ];
}
