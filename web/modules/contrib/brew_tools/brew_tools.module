<?php

/**
 * @file
 * Module file for Brew Tools
 * Contains \Drupal\system\Form\BrewToolsSettingsForm
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * test
 * Implements hook_help().
 */
function brew_tools_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.brew_tools':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t("A collection of tools for a home brewer.") . '</p>';
      return $output;
  }
}
