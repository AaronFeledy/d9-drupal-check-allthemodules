<?php

/**
 * @file
 * Module file for Remove meta tag headers reponse for Generator/X-Generator.
 */

/**
 * Implements hook_install().
 */
function remove_meta_and_headers_install() {

  // Set default values for config which require dynamic values.
  \Drupal::configFactory()->getEditable('remove_meta_and_headers.settings')
    ->set('meta_generator', 1)
    ->set('response_header_x_generator', 1)
    ->save();
}
