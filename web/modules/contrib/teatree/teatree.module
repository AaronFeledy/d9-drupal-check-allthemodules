<?php

/**
 * @file
 * Contains teatree.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function teatree_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the teatree module.
    case 'help.page.teatree':
      $output = '';
      $output .= '<h3>' . t('About TeaTree') . '</h3>';
      $output .= '<p>' . t('TeaTree is a Drupal 8 module for providing content as a service.') . '</p>';
      return $output;

    default:
  }
}
