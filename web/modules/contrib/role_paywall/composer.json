{
  "name": "drupal/role_paywall",
  "type": "drupal-module",
  "description": "Paywall for content based on user roles and fields.",
  "keywords": ["Drupal"],
  "license": "GPL-2.0+",
  "homepage": "https://netzstrategen.com",
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "netzstrategen team",
      "homepage": "http://netzstrategen/sind"
    }
  ],
  "require": {
    "cweagans/composer-patches": "^1.6",
    "drupal/flag": "^4.0-alpha3",
    "zzzzzzzzzzzzzzzzzzzzzzzzzzzzzzz/no-trailing-comma": "*"
  },
  "extra": {
    "enable-patching": true,
    "patches": {
      "drupal/flag": {
        "Flag service should be able to create a flagging with custom timestamp": "https://www.drupal.org/files/issues/2018-07-26/2988452-1-flag-service-store-created-timestamp.patch"
      }
    }
  }
}
