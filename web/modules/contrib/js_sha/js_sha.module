<?php

/**
 * @file
 * Provides integration with jsSHA, a JavaScript implementation of SHA and HMAC.
 */

/**
 * Implements hook_preprocess_html().
 */
function js_sha_preprocess_html(array &$variables) {
  // Ensure our necessary scripts are loaded to the page.
  $variables['#attached']['library'][] = 'js_sha/js_sha.sha';
}
