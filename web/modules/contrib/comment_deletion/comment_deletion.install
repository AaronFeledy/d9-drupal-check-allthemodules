<?php
/**
 * @file
 * Install, uninstall, and update functions for comment_deletion module.
 */

/**
 * Implements hook_uninstall().
 */
function comment_delete_uninstall() {
  // Delete variables.
  $site_config = \Drupal::configFactory()->getEditable('alert_to_admin.settings');
  $site_config->delete('comment_delete_types');
}
