<?php

/**
 * @file
 * Contains float_big.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function float_big_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the float_big module.
    case 'help.page.float_big':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Foat big size Module') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function float_big_theme() {
  $theme = [];

  return $theme;
}
