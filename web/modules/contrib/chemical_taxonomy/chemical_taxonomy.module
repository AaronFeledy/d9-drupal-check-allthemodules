<?php

/**
 * @file
 * Chemical taxonomy.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function chemical_taxonomy_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.chemical_taxonomy':
      $output = '';
      $output .= '<p>' . t('This module helps to provide chemical taxonomy.') . '</p>';
      $output .= '<p>' . t(".") . '</p>';
      return $output;
  }
}
