<?php

/**
 * @file
 * Partially Multilingual module install/update/uninstall code.
 */

/**
 * Add a configuration setting for the redirect code.
 */
function partial_multi_update_8001() {
  $config_factory = \Drupal::configFactory();
  $config = $config_factory->getEditable('partial_multi.settings');
  $config->set('redirect_code', 301);
  $config->save(TRUE);
}
