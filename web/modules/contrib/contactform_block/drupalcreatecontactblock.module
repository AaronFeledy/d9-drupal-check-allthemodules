<?php

/**
 * @file
 * Contains drupalcreatecontactblock.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function drupalcreatecontactblock_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the drupalcreatecontactblock module.
    case 'help.page.drupalcreatecontactblock':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Drupalcreate Contact Block') . '</p>';
      return $output;

    default:
  }
}
