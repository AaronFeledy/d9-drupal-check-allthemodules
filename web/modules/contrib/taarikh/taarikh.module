<?php

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function taarikh_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the taarikh module.
    case 'help.page.taarikh':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Taarikh') . '</p>';
      return $output;

    default:
  }
}
