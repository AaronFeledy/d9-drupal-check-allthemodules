{
    "name": "drupal/config_split_manager",
    "type": "drupal-module",
    "description": "Allows administrator to select modules per ennvironment and those will get exported to separate directories when exporting with 'drush csmex -y' command",
    "keywords": ["Drupal", "configuration", "configuration management"],
    "authors": [
        {
            "name": "Atul Kamboj",
            "email": "atulkamboj@gmail.com",
            "homepage": "https://www.drupal.org/u/atulkamboj",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": ">=5.6.0"
    },
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9"
            }
        }
    }
}
