<?php

/**
 * @file
 * Contains private_page.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;
use Drupal\Core\Url;

/**
 * Implements hook_help().
 */
function private_page_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.private_page':
      $output = '';
      $output .= '<h3>' . t('Private page') . '</h3>';
      $output .= '<p>' . t('Make private pages.') . '</p>';
      return $output;
  }
}

/**
 * Implements hook_element_info_alter().
 */
function private_page_element_info_alter(array &$types) {
  if (isset($types['toolbar'])) {
    $types['toolbar']['#attached']['library'][] = 'private_page/icon';
  }
}
