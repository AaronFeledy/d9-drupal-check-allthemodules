<?php

/**
 * @file
 * Contains rollback.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function rollback_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the rollback module.
    case 'help.page.rollback':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Adds functionality to rollback database updates') . '</p>';
      return $output;

    default:
  }
}
