<?php
/**
 * @file
 * Yaml Sandbox. For playing with Yaml.
 */

/**
 * Implements hook_help().
 */
function yaml_sandbox_help($route_name, \Drupal\Core\Routing\RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'yaml_sandbox.page':
      return '<p>' . t('Type YAML into the textarea below and then submit the form to see how that YAML will be converted to PHP.') . '</p>';
      break;
  }
}
