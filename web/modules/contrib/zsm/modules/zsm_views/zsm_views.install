<?php
/**
 * @file
 */

use Drupal\Core\Config\ConfigFactoryOverrideInterface;

/**
 * Implements hook_install().
 */
function zsm_views_install() {
    \Drupal::service('config.installer')->installDefaultConfig('module', 'zsm_views');
}

/**
 * Implements hook_uninstall().
 */
function zsm_views_uninstall() {
    \Drupal::service('config.manager')->uninstall('module', 'zsm_views');
}
