<?php

/**
 * @file
 * Commerce License Access Control module file.
 */

/**
 * Implements hook_uninstall().
 */
function commerce_license_access_control_uninstall() {
  commerce_license_access_control_enabled(FALSE);
}

/**
 * This is also required by ACL module.
 *
 * @see acl_node_access_records()
 */
function commerce_license_access_control_enabled($set = NULL) {
  // Not drupal_static!
  static $enabled = TRUE;
  if ($set !== NULL) {
    $enabled = $set;
  }
  return $enabled;
}
