<?php

/**
 * @file
 * Contains redirecton403.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function redirecton403_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the redirecton403 module.
    case 'help.page.redirecton403':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Redirects to custom login path') . '</p>';
      return $output;

    default:
  }
}
