<?php

/**
 * @file
 * This is the module to create a Pexels integration for Stock Photo field.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function stock_photo_pexels_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.stock_photo_pexels':
      $output = '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Pexels integration for Drupal API access to Pexels stock photo content.') . '</p>';

      return $output;
  }
}
