<?php

/**
 * @file
 * Contains sms_vianett.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function sms_vianett_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the sms_vianett module.
    case 'help.page.sms_vianett':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Integrates VIanett with the SMS Framework') . '</p>';
      return $output;

    default:
  }
}
