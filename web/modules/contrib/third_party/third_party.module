<?php

/**
 * @file
 * Provides an API for managing third party settings.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function third_party_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.third_party':
      $output = '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provides an API for managing third party settings.') . '</p>';
      return $output;
  }
}
