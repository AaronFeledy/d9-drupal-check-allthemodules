<?php

/**
 * @file
 * Contains views_render_view.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function views_render_view_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the views_render_view module.
    case 'help.page.views_render_view':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Render a view in a view field.') . '</p>';
      return $output;

    default:
  }
}
