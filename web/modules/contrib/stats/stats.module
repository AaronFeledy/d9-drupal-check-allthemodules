<?php

/**
 * @file
 * Contains stats.module.
 */

function _stats_run_executor(\Drupal\Core\Entity\EntityInterface $entity) {
  if (!$entity instanceof \Drupal\Core\Entity\ContentEntityInterface) {
    return;
  }

  /** @var \Drupal\stats\StatsExecutor $executor */
  $executor = \Drupal::service('stats.executor');
  $executor->execute($entity);
}

function _stats_debug() {
  /** @var \Drupal\kswertung\Entity\StatEntity $stat */
  $stat = \Drupal\kswertung\Entity\StatEntity::load(631);

  _stats_run_executor($stat);
}
