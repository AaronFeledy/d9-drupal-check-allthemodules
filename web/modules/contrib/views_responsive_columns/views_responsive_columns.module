<?php

/**
 * @file
 * Views responsive columns module help and theme functions.
 */

/**
 * @file
 * Implements hook_theme().
 */
function template_preprocess_views_responsive_columns(&$variables) {

  $variables['options'] = $variables['view']->style_plugin->options;
  $variables['#attached']['library'][] = 'views_responsive_columns/default';
  $variables['#attached']['drupalSettings']['viewsResponsiveColumns'] = $variables['view']->style_plugin->options;

}
