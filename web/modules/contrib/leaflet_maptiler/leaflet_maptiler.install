<?php

/**
 * @file
 * The install and update code for the leaflet_maptiler module.
 *
 * @ingroup leaflet_maptiler
 */

/**
 * Implements hook_uninstall().
 */
function leaflet_maptiler_uninstall() {
  /*
   * Deletes leaflet_maptiler configurations.
   */
  \Drupal::configFactory()->getEditable('leaflet_maptiler.settings')->delete();
}
