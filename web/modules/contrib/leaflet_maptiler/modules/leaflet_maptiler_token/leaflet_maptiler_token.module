<?php

/**
 * @file
 * Contains leaflet_maptiler_token.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function leaflet_maptiler_token_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the leaflet_maptiler_token module.
    case 'help.page.leaflet_maptiler_token':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provides tokens for Leaflet Maptiler') . '</p>';
      return $output;

    default:
  }
}
