<?php

/**
 * @file
 * Contains micro_webform.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;
use Drupal\Core\Form\FormStateInterface;

/**
 * Implements hook_theme().
 */
function micro_webform_theme() {
  $themes = [];

  $themes['webform_handler_micro_webform_email_summary'] = [
    'variables' => [
      'settings' => NULL,
      'handler' => NULL,
    ]
  ];

  return $themes;
}
