{% set return_class = single_value ? item_class : list_class %}
{% set get_method = single_value ? 'getFieldFirst' : 'getField' %}
  /**
   * @WrappersDelightMethod(
   *   field = "{{ field_name }}",
   *   type = WrappersDelightMethod::GETTER
   * )
   *
   * @return {{ return_class }}|NULL
   */
  public function {{ method_name }}() {
    return $this->{{ get_method }}('{{ field_name }}');
  }
