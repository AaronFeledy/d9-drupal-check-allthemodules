<?php

/**
 * @file
 * Contains hipa.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function hipa_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the hipa module.
    case 'help.page.hipa':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Hide Path') . '</p>';
      return $output;

    default:
  }
}
