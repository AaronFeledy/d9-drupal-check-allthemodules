<?php

use Drupal\user\Entity\Role;

function _user_hash_init_permissions() {
  $role = Role::load(Role::AUTHENTICATED_ID);
  $role->grantPermission('use user_hash');
  $role->save();
}

/**
 * Initialize permissions.
 */
function user_hash_update_8101(&$sandbox) {
  _user_hash_init_permissions();
}
