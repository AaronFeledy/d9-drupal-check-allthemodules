<?php
/**
 * @file
 * All functions for the Selector SimpleFader module.
 */

/**
 * Implements hook_page_attachments_alter().
 */


function simplefader_page_attachments_alter(&$build) {
    $build['#attached']['library'][] = 'simplefader/simplefader';
    $build['#attached']['drupalSettings']['simplefader']['simplefader_selected_element'] =
        \Drupal::config('simplefader.settings')->get('simplefader_selected_element');
}
