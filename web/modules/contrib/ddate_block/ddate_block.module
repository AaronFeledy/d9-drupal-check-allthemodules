<?php

/**
 * @file
 * Provides configurable blocks displaying a Discordian date.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function ddate_block_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.ddate_block':
      return '<p>' . t('This module provides configurable blocks displaying a Discordian date.') . '</p>';
  }
}
