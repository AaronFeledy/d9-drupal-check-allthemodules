<?php

/**
 * @file
 * Module onlinepbx.module.
 */

use Drupal\onlinepbx\Hook\Theme;
use Drupal\onlinepbx\Hook\View;

/**
 * Implements hook_theme().
 */
function onlinepbx_theme() {
  return Theme::hook();
}

/**
 * Implements hook_view().
 */
function onlinepbx_entity_view(array &$build, $entity, $display, $view_mode) {
  View::hook($build, $entity, $view_mode);
}
