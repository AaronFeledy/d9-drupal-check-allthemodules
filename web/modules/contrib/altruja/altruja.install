<?php

/**
 * Add embed_type to the altruja block entity.
 */
function altruja_update_8001(&$sandbox) {
  \Drupal::entityTypeManager()->clearCachedDefinitions();
  $altruja_block = \Drupal::entityTypeManager()->getDefinition('altruja_block');
  \Drupal::entityDefinitionUpdateManager()->installEntityType($altruja_block);
}