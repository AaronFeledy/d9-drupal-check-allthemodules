<?php

/**
 * @file
 * Contains install, update, uninstall functions for migrate_report module.
 */

/**
 * Implements hook_install().
 */
function migrate_report_install() {
  \Drupal::configFactory()
    ->getEditable('migrate_report.config')
    ->set('report_dir', file_directory_temp())
    ->save();
}
