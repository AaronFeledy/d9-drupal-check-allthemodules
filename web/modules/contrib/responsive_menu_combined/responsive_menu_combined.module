<?php

/**
 * @file
 * Module file for the responsive menu combined module.
 *
 * Module allow you to combine multiple menus into the
 * responsive hamburger menu.
 */

/**
 * Implements hook_theme() to add the template definition.
 **/
function responsive_menu_combined_theme($existing, $type, $theme, $path) {
  return [
    'block__responsive_menu_combined' => [
      '#attached' => array(
        'library' => array(
          'responsive_menu_combined/responsive_menu_combined',
        ),
      ),
    ],
  ];
}
