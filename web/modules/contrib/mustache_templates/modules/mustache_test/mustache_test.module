<?php

/**
 * @file
 * The mustache_test module file.
 */

/**
 * Implements hook_mustache_templates().
 */
function mustache_test_mustache_templates() {
  $path = drupal_get_path('module', 'mustache_test') . '/templates/';
  return [
    'some_test_template' => ['file' => $path . 'some-test-template.mustache.tpl'],
    'test_list' => ['file' => $path . 'test-list.mustache.tpl'],
    'test_auto_increment' => ['file' => $path . 'test-auto-increment.mustache.tpl'],
  ];
}
