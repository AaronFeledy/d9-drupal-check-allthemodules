use Drupal\entitytools\EntityOriginalCache;

/**
 * Implements hook_entity_presave().
 */
function entitytools_entity_presave(Drupal\Core\Entity\EntityInterface $entity) {
  // Ugly kitten massacre hack until we have
  // https://www.drupal.org/project/paragraphs/issues/3013961
  EntityOriginalCache::preSave($entity);
}