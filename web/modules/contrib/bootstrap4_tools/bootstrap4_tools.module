<?php

/**
 * @file
 * Contains bootstrap4_tools.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function bootstrap4_tools_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the bootstrap4_tools module.
    case 'help.page.bootstrap4_tools':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Number of accompanying modules for Bootstrap 4 theme.') . '</p>';
      return $output;

    default:
  }
}
