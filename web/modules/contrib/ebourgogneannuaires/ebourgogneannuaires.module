<?php

/**
 * @file
 * Contains ebourgogneannuaires.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function ebourgogneannuaires_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the ebourgogneannuaires module.
    case 'help.page.ebourgogneannuaires':
      $output = '';
      $output .= '<h3>' . t('À propos') . '</h3>';
      $output .= '<p>' . t("Permet d'insérer des annuaires e-bourgogne.") . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function ebourgogneannuaires_theme() {
  $theme = [];

  return $theme;
}
