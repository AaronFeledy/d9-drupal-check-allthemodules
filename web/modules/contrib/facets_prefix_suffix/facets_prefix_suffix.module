<?php

/**
 * @file
 * Contains facets_prefix_suffix.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function facets_prefix_suffix_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the facets_prefix_suffix module.
    case 'help.page.facets_prefix_suffix':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Processor for enabling prefix and suffix on result items.') . '</p>';
      return $output;

    default:
  }
}
