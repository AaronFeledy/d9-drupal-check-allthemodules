/**
 * Implements hook_uninstall().
 */
function media_entity_jw_player_uninstall() {
  \Drupal::service('config.factory')->getEditable('media_entity_jw_player.settings')
  ->clear('jw_authKey')
  ->clear('jw_expiration_time')
  ->clear('jw_player_id')
  ->clear('thumbnail_destination')
  ->save();
}
