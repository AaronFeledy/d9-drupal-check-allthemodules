<?php

/**
 * @file
 * Contains login_register_path.module
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 *	Implements hook_help().
 */
function login_register_path_help($route_name, RouteMatchInterface $route_match) {
	switch ($route_name) {
		case 'help.page.login_register_path':
			$output = '';
			$output .= '<h3>' . t('Login Register Path') . '</h3>';
			$output .= '<p>' . t('Change login and register page url.') . '</p>';
			return $output;
		
		default:
	}
}