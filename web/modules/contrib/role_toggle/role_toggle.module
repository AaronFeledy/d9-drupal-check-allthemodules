<?php

use Drupal\role_toggle\RoleToggle;
use Drupal\user\RoleInterface;

/**
 * Implements hook_ENTITY_TYPE_delete() for role.
 */
function role_toggle_role_delete(RoleInterface $role) {
  RoleToggle::hookRoleDelete($role);
}

/**
 * Implements hook_toolbar().
 */
function role_toggle_toolbar() {
  return RoleToggle::toolbar();
}
