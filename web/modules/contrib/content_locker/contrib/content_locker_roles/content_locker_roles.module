<?php

/**
 * @file
 * Content locker roles module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function content_locker_roles_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.content_locker_roles':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t("Roles plugin. Implements a block which locks content by user permissions.") . '</p>';
      return $output;
  }
}
