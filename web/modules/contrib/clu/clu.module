<?php

/**
 * @file
 * Currently logged-in users module file.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function clu_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the custom module.
    case 'help.page.clu':
      $output = '';
      $output .= '<h3>' . t('Currently logged-in users') . '</h3>';
      $output .= '<p>' . t('List the currently loggedin users and have an option to make them logout by admin.') . '</p>';
      return $output;

    default:
  }
}
