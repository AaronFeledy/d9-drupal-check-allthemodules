<?php

/**
 * @file
 * Config Ignore Readonly module file.
 */

/**
 * Implements hook_config_readonly_whitelist_patterns().
 */
function config_ignore_readonly_config_readonly_whitelist_patterns() {
  $patterns = Drupal::configFactory()
    ->get('config_ignore.settings')
    ->get('ignored_config_entities');

  return $patterns ?: [];
}
