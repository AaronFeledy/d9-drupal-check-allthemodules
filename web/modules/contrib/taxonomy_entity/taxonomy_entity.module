<?php

/**
 * @file
 * Provides expanded entity APIs.
 */

/**
 * Implements hook_entity_type_build().
 */
function taxonomy_entity_entity_type_build(array &$entity_types) {
  $entity_types['taxonomy_term']->setFormClass('default', 'Drupal\taxonomy_entity\Form\TermForm');
  $entity_types['taxonomy_term']->setFormClass('delete', 'Drupal\taxonomy_entity\Form\TermDeleteForm');
  $entity_types['taxonomy_vocabulary']->setFormClass('default', 'Drupal\taxonomy_entity\Form\VocabularyForm');
}
