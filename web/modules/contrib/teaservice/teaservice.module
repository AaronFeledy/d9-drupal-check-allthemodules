<?php

/**
 * @file
 * Contains teaservice.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function teaservice_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the teaservice module.
    case 'help.page.teaservice':
      $output = '';
      $output .= '<h3>' . t('About TeaService') . '</h3>';
      $output .= '<p>' . t('TeaService is a versatile Drupal 8 module for content as a service.') . '</p>';
      return $output;

    default:
  }
}
