<?php

 /**
 * @file
 * Defines installation hooks for copyscape module.
 */

/**
 * Implements hook_uninstall().
 */
function copyscape_uninstall() {
  \Drupal::service('config.factory')->getEditable('copyscape.settings')->delete();
  \Drupal::service('config.factory')->getEditable('copyscape.content')->delete();
}
