<?php

/**
 * @file
 * Contains cumulio.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function cumulio_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the cumulio module.
    case 'help.page.cumulio':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Cumulio integration') . '</p>';
      return $output;

    default:
  }
}
