{
  "name": "drupal/smallads",
  "description": "Smallads",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "homepage": "https://www.drupal.org/project/smallads",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Matthew Slater",
      "email": "matslats@fastmail.com",
      "homepage": "http://matslats.net",
      "role": "Maintainer"
    }
  ],
  "require": {
     "drupal/chosen": "2.6",
     "drupal/geofield": "1.0",
     "drupal/geofield_map": "2.23",
     "drupal/leaflet": "1.0-beta21"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "harvesthq/chosen",
        "version": "1.8.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/harvesthq/chosen/releases/download/v1.8.7/chosen_v1.8.7.zip",
          "type": "zip"
        },
        "require": {
            "composer/installers": "^1.2.0"
        }
      }
    }
  ],
  "extra": {
    "installer-paths": {
        "libraries/{$name}": ["type:drupal-library"]
    }
  }
}
