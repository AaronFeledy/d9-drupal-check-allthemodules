<?php

/**
 * @file
 * Contains field_prefix.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function field_prefix_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the field_prefix module.
    case 'help.page.field_prefix':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Customize the field prefix or remove the default prefix &quot;field_&quot;') . '</p>';
      return $output;

    default:
  }

}
