<?php

/**
 * @file
 * Contains swish_payment.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function swish_payment_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the swish_payment module.
    case 'help.page.swish_payment':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Implement Swish Payment API') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function swish_payment_theme() {
  return [
    'swish_payment' => [
      'template' => 'swish_payment',
      'render element' => 'children',
    ],
  ];
}
