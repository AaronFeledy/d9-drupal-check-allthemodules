<?php

/**
 * @file
 * Contains excel_libxl.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function excel_libxl_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the excel_libxl module.
    case 'help.page.excel_libxl':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Excel functionality for Drupal (Commerce LibXL library for PHP)') . '</p>';
      return $output;

    default:
  }
}
