<?php

/**
 * @file
 * Contains webform_feedback.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function webform_feedback_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the webform_feedback module.
    case 'help.page.webform_feedback':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('This module provides a feedback button for your website which allows you to gather customer/client feedback.') . '</p>';
      return $output;

    default:
  }
}
