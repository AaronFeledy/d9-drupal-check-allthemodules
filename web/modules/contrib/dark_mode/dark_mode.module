<?php

/**
 * @file
 * Contains dark_mode.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function dark_mode_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the dark_mode module.
    case 'help.page.dark_mode':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Switching theme as per the time') . '</p>';
      return $output;

    default:
  }
}
