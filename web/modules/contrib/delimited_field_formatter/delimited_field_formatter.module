<?php

/**
 * @file
 * Contains delimited_field_formatter.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function delimited_field_formatter_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the delimited_field_formatter module.
    case 'help.page.delimited_field_formatter':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provides various formatting options for delimited fields.') . '</p>';
      return $output;

    default:
  }
}
