use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function tealeaf_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the tealeaf module.
    case 'help.page.tealeaf':
      $output = '';
      $output .= '<h3>' . t('About TeaLeaf') . '</h3>';
      $output .= '<p>' . t('TeaLeaf is a Drupal 8 module for consuming content as a service.') . '</p>';
      return $output;

    default:
  }
}

