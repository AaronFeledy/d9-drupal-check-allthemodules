<?php

/**
 * @file
 * Contains \Drupal\vex_message\vex_message.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 *
 * @inheritdoc
 */
function vex_message_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.vex_message':
      return check_markup(file_get_contents(dirname(__FILE__) . '/README.txt'));
  }
}
