<?php

/**
 * @file
 * Contains client_004.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function client_004_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.client_004':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('004 Client') . '</p>';
      return $output;

    default:
  }
}
