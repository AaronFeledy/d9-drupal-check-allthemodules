<?php

/**
 * @file
 * Contains advanced_scheduler.module.
 */

 use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function advanced_scheduler_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.advanced_scheduler':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t("This module allows you to publish or unpublish the workbench enabled content.") . '</p>';
      return $output;
  }
}