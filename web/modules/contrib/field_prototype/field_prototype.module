<?php

/**
 * Implements hook_form_FORM_ID_alter().
 */
function field_prototype_form_field_ui_field_storage_add_form_alter(&$form, \Drupal\Core\Form\FormStateInterface $form_state, $form_id)
{
    kint($form);
    $config = \Drupal::config('field_prototype.settings');

    $form['field-prototype'] = [
        '#type' => 'fieldset',
        '#attributes' => ['class' => ['form--inline', 'clearfix']],
        '#title' => t('Clone field instance'),
        '#weight' => 0
    ];

    $form['field-prototype']['field-prototype-entities'] = array(
        '#type' => 'select',
        '#title' => t('Entity type'),
        '#options' => $config->get('field_prototype_other_entity_type')
    );
}