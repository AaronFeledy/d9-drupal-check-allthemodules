<?php

/**
 * @file
 * Contains migrate_process_s3.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function migrate_process_s3_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the migrate_process_s3 module.
    case 'help.page.migrate_process_s3':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provides migration plugins for working with any S3 compatible service.') . '</p>';
      return $output;

    default:
  }
}
