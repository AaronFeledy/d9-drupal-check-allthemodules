<?php

/**
 * @file
 * Contains navigation_blocks.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function navigation_blocks_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the back_button module.
    case 'help.page.navigation_blocks':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provides some navigational blocks, like a generic back button that, given a certain entity type, bundle or url, takes the user back to a preferred location.') . '</p>';
      return $output;

    default:
  }

  return '';
}
