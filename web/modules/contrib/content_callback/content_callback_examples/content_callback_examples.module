<?php

/**
 * Implements hook_content_callback_info_alter().
 */
function content_callback_examples_content_callback_info_alter(array &$definitions) {
  foreach ($definitions as $id => $defintion) {
    if ($defintion['id'] == 'views_content_callback') {
      $definitions[$id]['class'] = 'Drupal\content_callback_examples\Plugin\ContentCallback\Alter';
      $definitions[$id]['has_options'] = TRUE;
    }
  }
}
