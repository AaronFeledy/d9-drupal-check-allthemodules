<?php

/**
 * @file
 * Contains streamy_ui.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function streamy_ui_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the streamy_ui module.
    case 'help.page.streamy_ui':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Provides an user interface for Streamy') . '</p>';
      return $output;

    default:
  }
}
