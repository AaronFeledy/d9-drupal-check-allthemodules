<?php

/**
 * @file
 * Contains social_auth_ok.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 * @param $route_name
 * @param RouteMatchInterface $route_match
 * @return string
 */
function social_auth_ok_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the social_auth_ok module.
    case 'help.page.social_auth_ok':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Social Auth integration for Ok.ru') . '</p>';
      return $output;

    default:
  }
}
