<?php

/**
 * @file
 * Contains views_order_by_delta.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function views_order_by_delta_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the order_by_delta module.
    case 'help.page.views_order_by_delta':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Order views items by the delta property of a related field.') . '</p>';
      return $output;

    default:
  }
}
