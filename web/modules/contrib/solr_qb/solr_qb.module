<?php

/**
 * @file
 * Contains solr_qb.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function solr_qb_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the solr_qb module.
    case 'help.page.solr_qb':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Test your Solr queries.') . '</p>';
      return $output;

    default:
  }
}

/**
 * Implements hook_theme().
 */
function solr_qb_theme() {
  $theme = [];

  return $theme;
}
