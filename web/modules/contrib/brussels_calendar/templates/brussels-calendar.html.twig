<div id="{{calendar_id}}"></div>

<script>

window.onload = function(){
	{
		var calendarEvents={{events|raw}};
		var calendarSettings={{settings|raw}};
		var calendar = new Calendar('#{{calendar_id}}', calendarEvents, calendarSettings, {{javascript_day_function}}, {{javascript_event_function}});
	}
}
</script>
