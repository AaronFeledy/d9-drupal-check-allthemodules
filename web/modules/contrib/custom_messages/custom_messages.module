<?php

/**
 * @file
 * Contains custom_messages.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function custom_messages_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the custom_messages module.
    case 'help.page.custom_messages':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Add ability to create custom messages in any page') . '</p>';
      return $output;

    default:
  }
}
