<?php

/**
 * @file
 * Install hooks for serve_plain_file.
 */

/**
 * Implements hook_uninstall().
 */
function serve_plain_file_uninstall() {
  \Drupal::configFactory()->getEditable('serve_plain_file.settings')->delete();
}

/**
 * Install default config.
 */
function serve_plain_file_update_8001() {
  \Drupal::service('config.installer')->installDefaultConfig('module', 'serve_plain_file');
}
