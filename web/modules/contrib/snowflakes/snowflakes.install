<?php

/**
 * @file
 * Install, update and uninstall functions for the Snowflakes module.
 */

/**
 * Set a default value for the new 'toggle_button' config setting.
 */
function snowflakes_update_8101() {
  \Drupal::configFactory()->getEditable('snowflakes.settings')->set('toggle_button', FALSE)
    ->save();
}
