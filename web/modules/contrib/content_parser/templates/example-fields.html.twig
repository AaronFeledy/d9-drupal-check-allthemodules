    {% if fields|length == 1 %}
      return ${{ fields[0].type }}; {% if fields[0].description %}// {{ fields[0].description }}{% endif %}
    {% else %}
      return [<br>
        {% for field in fields %}
      &nbsp;&nbsp;'{{ field.name }}' => ${{ field.type }}, {% if field.description %}// {{ field.description }}{% endif %}<br>
        {% endfor %}
      ];
    {% endif %}
  {% else %}
    return [ <br>
      {% for item in [1,2,3] %}
