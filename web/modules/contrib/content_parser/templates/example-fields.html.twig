        {% for field in fields %}
          {% if field.isMulti and field.fields %}
&nbsp;&nbsp;&nbsp;&nbsp;'{{ field.name }}' => [<br>
            {% for item in field.fields %}
  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;'{{ item.name }}' => ${{ item.type }}, {% if item.description %}// {{ item.description }}{% endif %}<br>          
            {% endfor %}
&nbsp;&nbsp;&nbsp;&nbsp;];<br>
          {% else %}
  &nbsp;&nbsp;&nbsp;&nbsp;'{{ field.name }}' => ${{ field.type }}, {% if field.description %}// {{ field.description }}{% endif %}<br>
          {% endif %}
        {% endfor %}
    &nbsp;&nbsp;],<br>
      {% endfor %}
    ];
  {% endif %}
