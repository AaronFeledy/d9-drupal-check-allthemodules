<?php

/**
 * @file
 * Contains wodby.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function wodby_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the wodby module.
    case 'help.page.wodby':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Integration of the wodby SDK.') . '</p>';
      return $output;

    default:
  }
}
