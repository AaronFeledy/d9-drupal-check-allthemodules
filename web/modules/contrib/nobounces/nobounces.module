<?php

/**
 * @file
 * Contains nobounces.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function nobounces_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the nobounces module.
    case 'help.page.nobounces':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('NoBounces integration') . '</p>';
      return $output;

    default:
  }
}
