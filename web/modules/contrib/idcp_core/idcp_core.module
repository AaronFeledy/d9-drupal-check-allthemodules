<?php

/**
 * @file
 * Contains core.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function core_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the core module.
    case 'help.page.core':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('The basic module for IDCP.') . '</p>';
      return $output;

    default:
  }
}
