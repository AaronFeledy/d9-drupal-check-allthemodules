fsahoteldeals.fsahoteldeals_add: 
  path: 'admin/config/fsahoteldeals/add' 
  defaults: 
    _form: '\Drupal\fsahoteldeals\Form\AddDeal' 
    _title: 'Add Deal'
  requirements: 
    _permission: 'add brand form'
       
fsahoteldeals.fsahoteldeals_list: 
  path: 'admin/config/fsahoteldeals/list' 
  defaults: 
    _controller: '\Drupal\fsahoteldeals\Controller\FsaHotelDealsController::DealList' 
    _title: 'List Deals'
  requirements: 
    _permission: 'list deals'
    
fsahoteldeals.fsahoteldeals_delete: 
  path: 'admin/config/fsahoteldeals/delete/{id}' 
  defaults: 
    _form: '\Drupal\fsahoteldeals\Form\DeleteDeal' 
    _title: 'Delete Deal'
  requirements: 
    _permission: 'delete deal form'
        
fsahoteldeals.fsahoteldeals_edit: 
  path: 'admin/config/fsahoteldeals/edit/{id}' 
  defaults: 
    _form: '\Drupal\fsahoteldeals\Form\EditDeal' 
    _title: 'Edit Deal'
  requirements: 
    _permission: 'edit deal form'
    
