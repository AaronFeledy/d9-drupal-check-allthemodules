<?php

/**
 * @file
 * Contains views_timestamp_aggregate.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function views_timestamp_aggregate_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the views_timestamp_aggregate module.
    case 'help.page.views_timestamp_aggregate':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Add granular aggregation to timestamp field') . '</p>';
      return $output;

    default:
  }
}
