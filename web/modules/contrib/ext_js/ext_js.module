<?php

/**
 * Implement hook_page_bottom to change page array.
 * @param array $page_bottom
 */
function ext_js_page_bottom(array &$page_bottom){
  $file = \Drupal::config('ext_js.settings')->get('files');
  $page_bottom['#attached']['drupalSettings']['ext_js']['files'] = $file;
  $page_bottom['#attached']['library'][] = 'ext_js/ext_js';
}
