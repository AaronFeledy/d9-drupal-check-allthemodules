<?php

/**
 * @file
 * instafeedjs module file.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function instafeedjs_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.instafeedjs':
      $output = '';
      $output .= '<h3>' . t('Instafeed Js') . '</h3>';
      $output .= '<p>' . t('Module integrates with the Instafeed.js which is a dead-simple way to add Instagram photos to your website. This module requires no JQuery, just plain javascript.') . '</p>';
      $output .= '<p>' . t('This module ship with the default block to display the instagram photos') . '</p>';
      return ['#markup' => $output];
  }
}
