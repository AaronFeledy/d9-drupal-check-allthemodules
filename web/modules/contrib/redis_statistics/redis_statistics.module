<?php

/**
 * @file
 * Contains redis_statistics.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function redis_statistics_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the redis_statistics module.
    case 'help.page.redis_statistics':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Use Redis as a backend for Statistics.') . '</p>';
      return $output;

    default:
  }
}
