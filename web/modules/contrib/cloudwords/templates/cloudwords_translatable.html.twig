{#
/**
 * @file cloudwords_translatable.html.twig
 * Default theme implementation to present Cloudwords translatable data.
 *
 * This template is used when viewing Cloudwords translatable pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_cloudwords_translatable()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('cloudwords_translatable') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
