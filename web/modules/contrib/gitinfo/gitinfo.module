<?php

/**
 * @file
 * GIT info module.
 */

/**
 * Check if a PHP function is available.
 *
 * @param string $func
 *   The function to check.
 *
 * @return bool
 *   TRUE or FALSE.
 */
function gitinfo_function_enabled($func) {
  return is_callable($func) && FALSE === stripos(ini_get('disable_functions'), $func);
}
