<?php

/**
 * @file
 * Instapage module file.
 */

/**
 * Implements hook_page_attachments().
 *
 * @param array $attachments
 */
function instapage_page_attachments(array &$attachments) {
  if (!\Drupal::currentUser()->hasPermission('administer instapage landing pages')) {
    return;
  }
  $attachments['#attached']['library'][] = 'instapage/insta-style';
}
