<?php

/**
 * @file
 * Contains lightspeed_ecom_example_webhooks.module..
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function lightspeed_ecom_example_webhooks_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the lightspeed_ecom_example_webhooks module.
    case 'help.page.lightspeed_ecom_example_webhooks':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Example integration of Lightspeed eCom Webhooks') . '</p>';
      return $output;

    default:
  }
}
