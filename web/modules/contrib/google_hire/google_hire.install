<?php

/**
 * @file
 * Install hooks for Google Hire module.
 */

/**
 * Implements hook_uninstall().
 */
function google_hire_uninstall() {
  // Remove the Google Hire account settings.
  \Drupal::configFactory()->getEditable('google_hire.settings')
    ->clear('google_hire_domain')
    ->save();
}
